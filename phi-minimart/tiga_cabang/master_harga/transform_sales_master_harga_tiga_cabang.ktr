        <jsScript_script>&#47;&#47;Varian harga harian
var modal_cabang;
var harga_berlaku_cabang;
var biaya_cabang;

&#47;&#47;Helper
var koefisien_harga;
var koefisien_modal;
var koefisien_biaya;

generator = new java.util.Random();

koefisien_harga = (generator.nextInt(10) - 4) &#47; 500;
koefisien_modal = (generator.nextInt(600) * (generator.nextInt(3) - 1) &#47; 600) + 95;
koefisien_biaya = generator.nextInt(50) &#47; 2000;

harga_berlaku_cabang = harga_berlaku * (1 + koefisien_harga);
modal_cabang = harga_berlaku_cabang * koefisien_modal &#47; 100;
biaya_cabang = harga_berlaku_cabang * koefisien_biaya;
</jsScript_script>
